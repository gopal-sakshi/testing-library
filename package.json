{
  "name": "testing-lib",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --port 9997",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "serve:app1": "ng serve --project app1",
    "build:app1": "ng build --prod --project app1 --statsJson",
    "sourceMap:app1": "ng build --source-map=true",
    "analyse:app1": "webpack-bundle-analyzer dist/app1/stats.json",
    "bundle:app1": "source-map-explorer dist/app1/*.js",
    "serve:app2": "ng serve --project app2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/cdk": "^13.2.1",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^13.2.1",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "aws-amplify": "^4.3.34",
    "gopal612-lib-two": "0.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.16",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^12.1.1",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.5.0",
    "source-map-explorer": "2.5.2"
  }
